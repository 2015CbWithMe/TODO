.container{
  padding:50px 0 0 0;
  overflow-x: hidden;
}
.inp{
  width:80%;
  margin-bottom:20px;
}
.c-input{
  width:69%;
 
}
.usertype{
 float:left;
  width:100%;
  height:30px;
  text-align: left;
  padding-left:10px;
  color:#000;
  font-size:14px;
}
.addbtn{

  width:80px;
   background-color:#000;
   color:#fff;
   margin:0;
   height:32px;
   line-height: 30px;
   font-size:14px;
   border-radius:0 !important;
   

}
.lists{
  z-index:5;
 position:relative;
  border-bottom:1px solid #9a9a9a;
  padding:15px 20px;
  font-size:16px;
  overflow-x:hidden;
  background-color:#fff;
}
.radiowrap{
    position:relative;
    display:inline-block;
}
 .undercheckbox{
   opacity: 0;
   position:relative;
   width:20px;
  height:20px;
  z-index:1;
} 
.checkicon{
  position:absolute;
  width:25px;
  height:25px;
  left: 0;
  z-index:2
}
.hlist{
  width:100%;
  padding:0 20px;
  margin-top:20px;
}
.hlist>.hl-items{
    width:30%;
    border:1px solid #000;
    float:left;
    padding:5px 5px;
    text-align:center;
    font-size:14px;
}
.active{
  background-color:#000;
  color:#fff;
}
.selcetall{
    width:100%;
    height:50px;
    padding:5px 20px;
    line-height: 40px;
    border-bottom:1px solid #9a9a9a;
}
.showca{
    float:right;
    position:relative
}
.sabtn>.checkicon{
  top:10px;
}

.middlemg{
   margin:0 5%;
}
.selectAction{
  z-index:99;
  position:fixed;
  bottom:0;
  width:100%;
  height:50px;
  background-color:#272727;
  text-align:center;
  color:#fff;

}
.cawrap{
  float:left !important;
}
.selectAction view{
  width:50%;
  float: left;
    line-height: 50px;
  border-right:1px solid #414141;
}
.selectAction{
    transform:translate3d(0,50px,0);
    transition:.4s
}
.sabtn{
  position:relative;
  float: left;
}
.checkover{
  float:right;
}

 .ing{
      transform:translate3d(0,0,0) !important;
} 

.bd .tabbd{
      position:absolute;
      width:100%;
      transform:translate3d(-100%,0,0);
      transition: transform .4s; 
  
}

 .ing~.tabbd{
 transform:translate3d(100%,0,0);
}  
.listwrap{
  position:relative;
  overflow: hidden;
}
.slidebtn{
  z-index:4;
  position:absolute;
  width:0;
  top:0;
  right:0;

}
.sbbtn{
  position:relative;
  display:block !important;
width:100% !important;

}
.slidebtn view{
  display: inline-block;
  width:50%;
  color:#fff;
  text-align: center;
  padding:4px 0;
}
.donecur{
 background-color:#4abd0f;  
}
.delcur{
  background-color:#fb3434;


}
.donedelbtn view{
  width:100%;
}